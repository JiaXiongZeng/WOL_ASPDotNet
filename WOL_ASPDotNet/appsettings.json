{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SQLite": "Data Source=PCs.db",
    "KeyRing": "Data Source=KeyRings.db"
  },
  /*
     "Redis": Use Redis as session state repository
     "InProc": Use process memory resource as session state repository
  */
  "SessionMode": "InProc",
  "RedisSettings": {
    "Host": "localhost:6379",
    "InstanceName": "AspCoreWOL"
  },
  "AllowedHosts": "*",
  "Vite": {
    "Manifest": "vite-manifest.json"
  }
}
